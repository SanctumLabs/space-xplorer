  "scripts": {
    "start": "npm-run-all --parallel start:client start:server",
    "start:client": "node client/scripts/start.js",
    "start:server": "node server/build/server.js",
    "serve": "npm-run-all --parallel serve:client serve:server",
    "serve:client": "http-server ./client/build -p 8080",
    "serve:server": "node build/server.js",
    "build": "npm-run-all --parallel build:client build:server",
    "prebuild:client": "./client/env.sh; cp client/env-config.js client/public",
    "build:client": "sh -ac '. ./.env; node client/scripts/build.js'",
    "build:server": "cd server; webpack --optimize-minimize",
    "clean": "npm-run-all --parallel clean:client clean:server",
    "clean:client": "rm -rf client/build",
    "clean:server": "rm -rf server/build",
    "codegen": "apollo client:codegen --target typescript --watch",
    "test": "jest --runInBand",
    "test:coverage": "npm run test -- --coverage --watchAll=false",
    "test:watch": "jest --watch",
    "test:client": "node client/scripts/test.js --runInBand --updateSnapshot",
    "run:ios-demo": "npx artillery run apollo-internal-demos/ios-workload.yml",
    "run:android-demo": "npx artillery run apollo-internal-demos/android-workload.yml",
    "stylelint:fix": "stylelint 'client/src/**/*.{scss,sass}' --fix",
    "lint": "eslint '(client|server)/src/*/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint '(client|server/)/src/*/**/*.{js,jsx,ts,tsx}' --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
